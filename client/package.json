{
  "name": "client-series",
  "version": "1.0.0",
  "description": "The client series frontend based in React",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "clean:modules": "rm -rf node_modules",
    "coverage:open": "open coverage/lcov-report/index.html",
    "prepush": "npm run test && npm run build",
    "precommit": "npm run flow && lint-staged",
    "commit": "git-cz",
    "flow": "flow",
    "dev": "node config/browser-open.js && node config/webpack-server.js",
    "prebuild": "rm -rf ./public",
    "build": "webpack -p --config ./config/webpack.prod.config.js",
    "postbuild": "mv public/static/index.html public/index.html && npm run copy:files",
    "copy:files": "rm -rf ./../server/public && cp -r public ./../server/public",
    "serve:prod": "http-server public -p 8080",
    "test": "jest --config ./jest.config.json",
    "test:watch": "jest --watchAll --coverage --config ./jest.config.json",
    "lint:js": "eslint ./src/**/*.js --color",
    "lint:css": "stylelint 'src/**/*.scss'",
    "generate:stateful": "node ./scripts/stateful.js",
    "generate:stateless": "node ./scripts/stateless.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write --single-quote",
      "git add"
    ],
    "*.scss": "stylelint"
  },
  "config": {
    "commitizen": {
      "path": "client/node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [],
  "author": "Gabriel Garc√≠a Seco",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.10",
    "cypress": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.71.0",
    "genmite": "^1.1.6",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "open": "0.0.5",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.12.1",
    "react-test-renderer": "^16.3.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.0.0",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.8.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
